<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.powerboot.dao.WindowContentDao">

    <select id="getContent" resultType="com.powerboot.domain.WindowContentDO">
        select
            `title`,
            `content`
        from window_content
        order by sort desc
    </select>


</mapper>