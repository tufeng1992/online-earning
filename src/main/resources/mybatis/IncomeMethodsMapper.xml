<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.powerboot.dao.IncomeMethodsDao">

    <select id="getContent" resultType="com.powerboot.domain.IncomeMethodsDO">
        select
          `title`,
          `show_red`,
          `content`
        from income_methods
        order by sort desc
    </select>


</mapper>